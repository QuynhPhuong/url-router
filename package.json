{
  "name": "url-router",
  "version": "4.0.0",
  "description": "URL Router",
  "main": "dist/Router.js",
  "scripts": {
    "dev": "babel src -w -d dist",
    "build": "babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fenivana/Router.git"
  },
  "keywords": [
    "url",
    "router"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fenivana/Router/issues"
  },
  "homepage": "https://github.com/fenivana/Router",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.5.0",
    "eslint-config-enough": "^0.2.8"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true,
          "modules": "umd"
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true
    }
  }
}
