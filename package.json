{
  "name": "url-router",
  "version": "2.0.0",
  "description": "URL Router",
  "main": "dist/UrlRouter.js",
  "scripts": {
    "dev": "babel src -w -d dist",
    "build": "babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fenivana/Router.js.git"
  },
  "keywords": [
    "url",
    "router"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fenivana/Router.js/issues"
  },
  "homepage": "https://github.com/fenivana/Router.js",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "eslint": "^3.13.1",
    "eslint-config-enough": "^0.2.4"
  },
  "babel": {
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "loose": true,
            "modules": "umd"
          }
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true
    }
  }
}
