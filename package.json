{
  "name": "url-router",
  "version": "6.0.0",
  "description": "A non-opinionated cross-platform URL routing library.",
  "main": "dist/Router.umd.js",
  "module": "dist/Router.esm.js",
  "scripts": {
    "build": "rollup -c rollup.config.esm.js && rollup -c rollup.config.umd.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fenivana/Router.git"
  },
  "keywords": [
    "url",
    "router"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fenivana/Router/issues"
  },
  "homepage": "https://github.com/fenivana/Router",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.5.0",
    "eslint-config-enough": "^0.3.1",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true,
          "modules": false
        }
      ]
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true
    }
  }
}
