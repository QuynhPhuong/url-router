{
  "name": "url-router",
  "version": "7.1.5",
  "description": "A non-opinionated cross-platform URL routing library.",
  "main": "dist/URLRouter",
  "scripts": {
    "build": "rollup -c rollup.config.esm.js && rollup -c rollup.config.cjs.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jiangfengming/url-router.git"
  },
  "keywords": [
    "url",
    "router"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jiangfengming/url-router/issues"
  },
  "homepage": "https://github.com/jiangfengming/url-router",
  "dependencies": {
    "cast-string": "^0.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.2.1",
    "eslint-config-enough": "^0.3.6",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.3.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "modules": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true
    }
  }
