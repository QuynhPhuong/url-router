{
  "name": "url-router",
  "version": "7.1.0",
  "description": "A non-opinionated cross-platform URL routing library.",
  "main": "dist/Router",
  "scripts": {
    "build": "rollup -c rollup.config.esm.mjs && rollup -c rollup.config.umd.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jiangfengming/url-router.git"
  },
  "keywords": [
    "url",
    "router"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jiangfengming/url-router/issues"
  },
  "homepage": "https://github.com/jiangfengming/url-router",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.1",
    "eslint-config-enough": "^0.3.4",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "modules": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true
    }
  }
}
