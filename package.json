{
  "name": "url-router",
  "version": "6.0.1",
  "description": "A non-opinionated cross-platform URL routing library.",
  "main": "dist/Router.umd.js",
  "module": "dist/Router.esm.js",
  "scripts": {
    "build": "rollup -c rollup.config.esm.js && rollup -c rollup.config.umd.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fenivana/url-router.git"
  },
  "keywords": [
    "url",
    "router"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fenivana/url-router/issues"
  },
  "homepage": "https://github.com/fenivana/url-router",
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "babel-eslint": "^8.2.6",
    "eslint": "^5.4.0",
    "eslint-config-enough": "^0.3.4",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^4.0.0-beta.8"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "modules": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true
    }
  }
}
